#include<iostream>
#include<vector>
using namespace std;
int main(){
    //initialising first array
    int arr[]={1,3,5,7};
    int size_arr=4;
    //initialising second array
    int brr[]={2,4,6,8};
    int size_brr=4;
    //declaring a ans array
    vector<int>ans;
    //pushing elements of arr array into ans
    for(int i=0;i<size_arr;i++){
        ans.push_back(arr[i]);

    }
    //pushing elements of brr array into ans
    for(int i=0;i<size_brr;i++){
        ans.push_back(brr[i]);
    }
    //printing elements of ans array
    for(int i=0;i<ans.size();i++){
        cout<<ans[i]<<" ";

    }
    return 0;

}
